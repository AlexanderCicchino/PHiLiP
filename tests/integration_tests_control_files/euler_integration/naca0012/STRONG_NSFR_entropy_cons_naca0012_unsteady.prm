# Listing of Parameters                                                                                                                                                                                            
# ---------------------

set test_type = naca0012_unsteady_check_quick
set dimension = 2 

set use_weak_form = false
set pde_type  = euler
set use_split_form = true
set two_point_num_flux_type = Ra
set conv_num_flux = two_point_flux
set overintegration = 10 
set output_face_results_vtk = true

subsection euler
  set reference_length = 1.0 
  set mach_infinity = 0.50
  set angle_of_attack = 2.0 
end

subsection ODE solver
  set output_solution_every_x_steps = 10
  set ode_solver_type  = runge_kutta
  set print_iteration_modulo              = 1
end

subsection flow_solver
  set flow_case_type = naca0012
  set steady_state = false
  set constant_time_step = 0.0001
  set final_time = 0.001
  subsection grid
    set input_mesh_filename = ../../../meshes/naca0012_hopw_ref2
  end
end
