# Listing of Parameters
# ---------------------
# Number of dimensions
set dimension = 3

set pde_type = euler

subsection euler
  set mach_infinity = 0.1
end

set test_type = flow_solver
subsection flow_solver
  set flow_case_type = inviscid_taylor_green_vortex
end

# Grid parameters + Polynomial Order of Solution
subsection grid refinement study
  set num_refinements = 0
  set poly_degree = 3
  set grid_type = hypercube
  # boundaries
  set grid_left = 0.0
  set grid_right = 6.283185308
  # number of elements per side
  set grid_size = 16
end
